<!DOCTYPE html>
<html lang='en'>
<head>
  <meta charset='UTF-8'>
  <title>Web Widget Chat History</title>
</head>
<body>
  <h1>Web Widget Chat History</h1>
  <%= htmlWebpackPlugin.options.snippet %>
  <script nonce="<%= htmlWebpackPlugin.options.nonce %>">
    zE(function() {
      zE.devRender({
        webWidgetCustomizations: true,
        newChat: true,
        embeds: {
          launcher: {
            embed: 'launcher',
            props: {
              position: 'right'
            }
          },
          zopimChat: {
            embed: 'chat',
            props: {
              zopimId: '<%= htmlWebpackPlugin.options.zopimId %>'
            }
          }
        }
      });
    });

    window.zESettings = {
      authenticate: {
        chat: {
          jwtFn: (cb) => cb('<%= htmlWebpackPlugin.options.chatJwt %>')
        }
      }
    };
  </script>
</body>
</html>
