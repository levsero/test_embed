{
  "predef":    [
    "module", "require", "requireUncached",
    "process",
    "React",
    "zE",
    "zEmbed",
    "_",
    "Airbrake",
    "before", "after", "noop", "pluckSubscribeCall", "resetDOM", "ReactTestUtils", "jsdom", "dispatchEvent", "noopReactComponent",
    "expect", "it", "describe", "beforeEach", "afterEach",
