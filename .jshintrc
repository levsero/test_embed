  "quotmark":  "single",
  "trailing":  true,
  "regexp":    false,
  "undef":     true,
  "unused":    true,
  "white":     false,
  "scripturl": true,

  "debug":     false,
  "evil":      false,
  "forin":     false,
  "laxbreak":  false,
  "sub":       false,

  "indent":     2,
  "maxerr":     50,
  "passfail":   false,
  "browser":    true,
  "node":       true,
  "rhino":      false,
  "devel":      true,
  "lastsemic":  false,
  "expr":       true,
  "esnext":     true
