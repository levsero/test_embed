{
  "env": { "jasmine": true },
  "plugins": ["jasmine"],
  "globals": {
    "_": true,
    "addCustomCommands": true,
    "basePath": true,
    "buildTestPath": true,
    "buildSrcPath": true,
    "connectedComponent": true,
    "domRender": true,
    "initMockRegistry": true,
    "jsdom": true,
    "mockery": true,
    "noop": true,
    "noopReactComponent": true,
    "scrollContainerComponent": true,
    "pluckSubscribeCall": true,
    "React": true,
    "TestUtils": true,
    "ReactDOM": true,
    "requireUncached": true,
    "resetDOM": true,
    "shallowRender": true,
    "shallow": true,
    "instanceRender": true,
    "getRenderer": true,
    "testRender": true,
    "mockObjectDifference": true,
    "actionSpy": true
  },
  "rules": {
    "max-len": 0,
    "react/prop-types": 0,
    "jasmine/no-focused-tests": 2
  }
}
