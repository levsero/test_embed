    "rest": "~2.0.0",
    "rollbar-sourcemap-webpack-plugin": "^2.5.1",
    "sass-loader": "^7.0.1",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "source-map-loader": "^0.2.3",
    "svgo": "^0.7.2",
    "svgo-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-bundle-analyzer": "^3.3.2",
