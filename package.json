{
  "name": "engagement_framework",
  "version": "0.1.0",
  "description": "3rd party embeddable framework and API",
  "config": {
    "watchConfigPath": "/config/.watch.example"
  },
  "scripts": {
    "build:version:generate": "git rev-parse --short HEAD > ./dist/VERSION_HASH",
    "build:version:clean": "rm -rf ./dist/VERSION_HASH",
    "prebuild-examples": "uglifyjs --compress --mangle -- ./src/bootstrap.js > ./dist/bootstrap.js",
    "build-examples": "node script/build-examples.js -- $npm_package_config_watchConfigPath",
    "prewatch": "npm run build:version:generate && npm run build-examples",
    "build-setup:prod": "npm run build:version:generate && npm run prebuild-examples && rm -rf ./dist/*.map",
    "build:ze_translations": "uglifyjs -c -m -o dist/ze_translations.js -- src/translation/ze_translations.js",
    "build:ze_localeIdMap": "uglifyjs -c -m -o dist/ze_localeIdMap.js -- src/translation/ze_localeIdMap.js",
    "build:ze_countries": "uglifyjs -c -m -o dist/ze_countries.js -- src/translation/ze_countries.js",
    "prebuild:ac": "script/ac_fetch_i18n && cp manifest.json dist && npm run build:ze_translations && npm run build:ze_localeIdMap && npm run build:ze_countries",
    "build:prod": "script/fetch_i18n && webpack --config=./webpack/webpack.prod.config.js --progress",
    "build:ac": "webpack --config=./webpack/webpack.ac.config.js --progress",
    "watchConf": "node script/update_watch_config",
    "postwatchConf": "npm run watch",
    "watch": "webpack-dashboard -- webpack-dev-server --config=./webpack/webpack.dev.config.js",
    "watch:ac": "./script/start-ac-dev && npm run prewatch && webpack-dashboard -- webpack-dev-server --config=./webpack/webpack.ac.dev.config.js",
    "build": "npm run build-setup:prod && npm run build:prod",
    "build-ac": "npm run build-setup:prod && npm run build:ac",
    "test": "node script/jasmine-runner.js",
    "test:debug": "node debug script/jasmine-runner.js",
    "lint": "eslint src test --ignore-pattern=src/translation/*.js",
    "validate-manifest": "node script/validate_manifest.js"
  },
  "author": "Zendesk",
  "license": "All Rights Reserved",
  "dependencies": {
    "airbrake-js": "^0.5.9",
    "airwaves": "~0.3.0",
    "chat-web-sdk": "git+ssh://git@github.com/zendesk/chat-web-sdk.git#1.1.4",
    "color": "~0.7.3",
    "core-js": "^2.5.1",
    "libphonenumber-js": "~0.2.2",
    "lodash": "~4.0.0",
    "prop-types": "^15.5.8",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^2.6.1",
    "redux-on-state-change": "^1.0.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.1",
    "sanitize-html": "~1.11.4",
    "sha1": "^1.1.1",
    "socket.io-client": "^2.0.4",
    "sprintf-js": "^1.1.1",
    "superagent": "^2.0.0",
    "zd-css-buttons": "git+ssh://git@github.com/zendeskgarden/css-buttons.git#2.2.0",
    "zd-css-variables": "git+ssh://git@github.com/zendeskgarden/css-variables.git#0.11.0",
    "zd-svg-icons": "git+ssh://git@github.com/zendeskgarden/svg-icons.git#1.4.0"
  },
  "devDependencies": {
    "app-root-path": "^2.0.1",
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.20.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "~6.2.9",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.24.1",
    "chalk": "^1.1.3",
    "classnames": "^2.2.1",
    "concurrently": "^3.1.0",
    "css-loader": "^0.23.1",
    "cssstyle": "0.2.30",
    "dotenv": "^4.0.0",
    "es5-shim": "~3.4.0",
    "eslint": "^1.10.3",
    "eslint-plugin-jasmine": "^2.8.4",
    "eslint-plugin-react": "^3.11.3",
    "file-loader": "^0.8.5",
    "glob": "^6.0.1",
    "htmlmin": "0.0.4",
    "imports-loader": "~0.6.2",
    "inline-source": "^4.2.2",
    "jasmine": "^2.8.0",
    "jasmine-ajax": "git://github.com/pivotal/jasmine-ajax#v2.0.0.rc2",
    "jasmine-spec-reporter": "^2.4.0",
    "jsdom": "~7.2.2",
    "jsonwebtoken": "^5.7.0",
    "mockery": "~1.4.0",
    "node-cmd": "^3.0.0",
    "node-sass": "^3.4.2",
    "postcss-loader": "^1.3.3",
    "raw-loader": "~0.5.1",
    "react-shallow-testutils": "~3.0.1",
    "react-test-renderer": "^16.0.0",
    "react-transform-catch-errors": "^1.0.1",
    "redux-mock-store": "^1.2.1",
    "require-uncached": "~1.0.2",
    "rest": "~1.2.0",
    "run-sequence": "~0.3.6",
    "sass-loader": "~3.1.2",
    "selenium-server": "~2.43.1",
    "semver": "^5.3.0",
    "source-map-loader": "^0.2.1",
    "svgo": "~0.5.3",
    "svgo-loader": "~1.1.0",
    "tiny-lr": "0.0.7",
    "uglify-js": "^2.6.1",
    "url-loader": "~0.5.7",
    "webpack": "^2.4.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dashboard": "^0.2.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-visualizer-plugin": "^0.1.5",
    "when": "~3.4.4"
  }
}
