{
  "name": "engagement_framework",
  "version": "0.1.0",
  "author": "Zendesk",
  "license": "All Rights Reserved",
  "private": true,
  "description": "3rd party embeddable framework and API",
  "engines": {
    "nvm": ">= 0.35.2",
    "node": ">= 10.18.0",
    "npm": ">= 6.13.4"
  },
  "scripts": {
    "latest": "git checkout master && git pull && ./script/setup_node_env",
    "benchmark": "node ./script/lighthouse/generate_report.js",
    "build:version:generate": "git rev-parse --short HEAD > ./dist/VERSION_HASH",
    "build:version:clean": "rm -rf ./dist/VERSION_HASH",
    "predev": "npm run build:version:generate",
    "dev": "webpack-dev-server --config ./webpack/webpack.ac.dev.js --hot",
    "dashboard": "USE_DASHBOARD=true npm run dev",
    "dev-preview": "webpack-dev-server --config ./webpack/webpack.previewer.dev.js",
    "build-setup:prod": "npm run build:version:generate && rm -rf ./dist/*.map && script/fetch_i18n",
    "build:prod": "NODE_ENV=production webpack --config ./webpack/webpack.ac.prod.js",
    "build:validate": "npm run build:prod && npm run size",
    "build:previewer": "NODE_ENV=production webpack --config ./webpack/webpack.previewer.prod.js",
    "build": "npm run build-setup:prod && npm run build:prod",
    "build-previewer": " npm run prebuild && npm run build-setup:prod && npm run build:previewer",
    "test": "TZ=utc jest",
    "test:updateSnapshots": "TZ=utc jest --updateSnapshot",
    "e2e:server": "NODE_ENV=e2e webpack-dev-server --config ./webpack/webpack.e2e.js",
    "e2e": "NODE_ENV=e2e TZ=utc jest -c ./jest-puppeteer.config.js",
    "e2e:ci": "npm run e2e -- --runInBand --ci",
    "e2e:debug": "NODE_ENV=e2e TZ=utc node --inspect-brk node_modules/.bin/jest -c ./jest-puppeteer.config.js",
    "test:watch": "npm run test -- --watch",
    "test:old": "NODE_ENV=test node test/runner.js",
    "test:ci": "npm run test -- --runInBand --ci && NODE_ENV=test node test/runner.js",
    "test:performance": "node e2e/performance/performance.js",
    "test:debug:old": "node debug test/runner.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --no-cache --watch",
    "lint:js": "eslint *.js config dev e2e script src test webpack",
    "lint:other": "prettier --check \"**/*.{css,scss,json,md}\"",
    "lint": "npm run lint:js && npm run lint:other",
    "lint-fix:js": "npm run lint:js -- --fix",
    "lint-fix:other": "prettier --write \"**/*.{css,scss,json,md}\"",
    "lint-fix": "npm run lint-fix:js && npm run lint-fix:other",
    "size": "bundlesize",
    "prebuild": "rm -rf ./dist && mkdir dist",
    "postshrinkwrap": "replace --silent 'http://' 'https://' ./package-lock.json"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,scss,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "bundlesize": [
    {
      "path": "dist/public/preload.*.js",
      "maxSize": "20 KB"
    },
    {
      "path": "dist/public/vendors~web_widget*.js",
      "maxSize": "315 KB"
    },
    {
      "path": "dist/public/web_widget*.js",
      "maxSize": "200 KB"
    },
    {
      "path": "dist/public/lazy/*.js",
      "maxSize": "30 KB"
    },
    {
      "path": "dist/public/talk-sdk.*.js",
      "maxSize": "20 KB"
    },
    {
      "path": "dist/public/chat-sdk.*.js",
      "maxSize": "60 KB"
    }
  ],
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "last 2 Android versions",
    "last 2 ChromeAndroid versions",
    "IE 11"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@zendesk/client-i18n-tools": "^13.0.0",
    "@zendeskgarden/container-focusjail": "^1.2.2",
    "@zendeskgarden/container-modal": "^0.8.2",
    "@zendeskgarden/css-bedrock": "^7.0.35",
