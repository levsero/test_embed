{
  "name": "engagement_framework",
  "version": "0.1.0",
  "description": "3rd party embeddable framework and API",
  "engines": {
    "nvm": ">= 0.33.11",
    "node": ">= 8.9.4",
    "npm": ">= 6.5.0"
  },
  "scripts": {
    "latest": "git checkout master && git pull && ./script/setup_node_env",
    "build:version:generate": "git rev-parse --short HEAD > ./dist/VERSION_HASH",
    "build:version:clean": "rm -rf ./dist/VERSION_HASH",
    "predev": "npm run build:version:generate",
    "dev": "webpack-dev-server --config ./webpack/webpack.ac.dev.js",
    "dev-preview": "webpack-dev-server --config ./webpack/webpack.previewer.dev.js",
    "build-setup:prod": "npm run build:version:generate && rm -rf ./dist/*.map && script/fetch_i18n",
    "build:prod": "NODE_ENV=production webpack --config ./webpack/webpack.ac.prod.js",
    "build:validate": "npm run build:prod && npm run size",
    "build:previewer": "NODE_ENV=production webpack --config ./webpack/webpack.previewer.prod.js",
    "build": "npm run build-setup:prod && npm run build:prod",
    "build-previewer": "npm run build-setup:prod && npm run build:previewer",
    "test": "TZ=utc jest",
    "test:updateSnapshots": "TZ=utc jest --updateSnapshot",
    "e2e:server": "webpack-dev-server --config ./webpack/webpack.e2e.js",
    "e2e:ci": "NODE_ENV=ci TZ=utc jest -c ./jest-puppeteer.config.js",
    "e2e:dev": "NODE_ENV=dev TZ=utc jest -c ./jest-puppeteer.config.js",
    "e2e:debug": "NODE_ENV=dev TZ=utc node --inspect-brk node_modules/.bin/jest -c ./jest-puppeteer.config.js",
    "test:watch": "npm run test -- --watch",
    "test:old": "node test/runner.js",
    "test:ci": "npm run test -- --runInBand --ci && NODE_ENV=test node test/runner.js",
    "test:debug:old": "node debug test/runner.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --no-cache --watch",
    "lint:js": "eslint *.js config dev e2e script src test webpack",
    "lint:other": "prettier --check \"**/*.{css,scss,json,md}\"",
    "lint": "npm run lint:js && npm run lint:other",
    "lint-fix:js": "npm run lint:js -- --fix",
    "lint-fix:other": "prettier --write \"**/*.{css,scss,json,md}\"",
    "lint-fix": "npm run lint-fix:js && npm run lint-fix:other",
    "size": "bundlesize",
    "postshrinkwrap": "replace --silent 'http://' 'https://' ./package-lock.json"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,scss,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "bundlesize": [
    {
      "path": "dist/web_widget.*.js",
      "maxSize": "395 KB"
    },
    {
      "path": "dist/common_vendor.*.js",
      "maxSize": "120 KB"
    },
    {
      "path": "dist/talk_vendor.*.js",
      "maxSize": "55 KB"
    },
    {
      "path": "dist/chat_vendor.*.js",
      "maxSize": "60 KB"
    }
  ],
  "author": "Zendesk",
  "license": "All Rights Reserved",
  "dependencies": {
    "@zendesk/client-i18n-tools": "^10.6.3",
    "@zendeskgarden/css-arrows": "^3.1.0",
    "@zendeskgarden/css-bedrock": "^7.0.14",
    "@zendeskgarden/css-buttons": "6.2.0",
    "@zendeskgarden/css-tooltips": "^4.1.6",
    "@zendeskgarden/css-variables": "^5.2.3",
    "@zendeskgarden/react-buttons": "^3.7.0",
    "@zendeskgarden/react-checkboxes": "3.4.8",
    "@zendeskgarden/react-modals": "^5.4.0",
    "@zendeskgarden/react-notifications": "^3.2.0",
    "@zendeskgarden/react-select": "^2.5.2",
    "@zendeskgarden/react-selection": "^4.6.2",
    "@zendeskgarden/react-textfields": "^3.7.0",
    "@zendeskgarden/react-theming": "^3.2.1",
    "@zendeskgarden/react-utilities": "^0.2.5",
    "@zendeskgarden/svg-icons": "~5.0.0",
    "airwaves": "~0.3.0",
    "async": "^2.6.1",
    "chat-web-sdk": "git+ssh://git@github.com/zendesk/chat-web-sdk.git#v23.0.0",
    "classnames": "^2.2.5",
    "color": "~3.0.0",
    "core-js": "^2.5.1",
    "libphonenumber-js": "~0.4.49",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.1",
    "re-reselect": "^2.2.0",
    "react": "^16.8.6",
