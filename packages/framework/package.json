    "re-reselect": "^2.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-final-form": "^6.3.0",
    "react-linkify": "^0.2.2",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.1.2",
    "react-slick": "^0.23.1",
    "react-transition-group": "^2.9.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
