{
  "extends": [
    "../../config/eslint/eslint-defaults.js",
    "../../config/eslint/eslint-es6.js",
    "../../config/eslint/eslint-react.js",
    "../../config/eslint/eslint-plugin-import.js",
    "plugin:prettier/recommended",
    "plugin:jsx-a11y/recommended",
