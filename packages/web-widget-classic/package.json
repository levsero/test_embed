{
  "name": "@zendesk/web-widget-classic",
  "version": "0.0.1",
  "author": "Zendesk",
  "license": "All Rights Reserved",
  "private": true,
  "engines": {
    "node": ">= 12.21.0",
    "npm": ">= 6.13.4"
  },
  "scripts": {
    "build:version:generate": "mkdir -p dist && git rev-parse --short HEAD > ./dist/VERSION_HASH",
    "build:version:clean": "rm -rf ./dist/VERSION_HASH",
    "build": "NODE_ENV=production npm run build:version:generate && webpack  --config ./webpack.prod.config.js",
    "dev": "NODE_ENV=development yarn run build:version:generate &&  webpack serve",
    "test": "jest",
    "e2e:server": "NODE_ENV=e2e webpack serve --config ./webpack.e2e.config.js"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/register": "^7.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^4.0.0",
    "lodash": "^4.17.20",
    "mockery": "~1.4.0",
    "react-test-renderer": "^16.8.6",
    "webpack-merge": "^5.8.0"
  },
  "peerDependencies": {},
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "last 2 Android versions",
    "last 2 ChromeAndroid versions",
    "IE 11"
  ],
  "sideEffects": false
}
