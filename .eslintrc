{
  "extends": [
    "./config/eslint/eslint-defaults.js",
    "./config/eslint/eslint-es6.js",
    "./config/eslint/eslint-react.js",
    "./config/eslint/eslint-plugin-import.js"
  ],
  "plugins": ["jest"],
  "root": true,
  "env": {
    "browser": true,
    "jest/globals": true
  },
  "globals": {
    "__DEV__": true,
    "__EMBEDDABLE_VERSION__": true,
    "addCustomCommands": true,
    "webdrivercss": true,
    "webdriverio": true,
    "Component": true,
    "mount": true,
    "noop": true
  },
  "rules": {
    "no-var": 0,
    "indent": [2, 2, {"SwitchCase": 1}]
   }
}
